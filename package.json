{
  "name": "react-cms-api",
  "version": "1.0.0",
  "description": "nodejs api connected to cms",
  "repository": "https://github.com/entymon/react-cms-api.git",
  "author": "entymon <pawel.olejniczak.i@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others --names \"tsc,node\" -c \"blue,green\" \"npx tsc --noEmit --pretty --watch\" \"npx nodemon api/server.ts --exec babel-node --extensions '.ts,.tsx' api/server.ts\"",
    "typecheck": "npx tsc --noEmit --pretty",
    "build": "npx tsc --noEmit --pretty && npx babel --extensions '.ts,.tsx' --source-maps --out-dir dist api",
    "serve": "node --require source-map-support/register dist/server.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/node": "^7.0.0-beta.42",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.42",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0-beta.42",
    "@babel/plugin-transform-typescript": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "concurrently": "^3.5.1",
    "nodemon": "^1.13.3",
    "prettier": "^1.9.2",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/express": "^4.11.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "flat": "^4.0.0",
    "nosql": "^6.1.0",
    "redis": "^2.8.0",
    "source-map-support": "^0.5.0",
    "tslib": "^1.8.1",
    "uuid": "^3.2.1"
  }
}
